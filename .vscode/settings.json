{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "cSpell.words": [
    "gtag",
    "heroicons",
    "microcms",
    "semibold"
  ],
  "css.validate": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "cSpell.ignoreWords": [
    "browserconfig",
    "msapplication"
  ]
}
